{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "quotes": ["error", "single"],
    "prefer-const": "error",
//    "indent": ["warn", 2],
    "max-len": ["error", { "code": 120 }],
    "semi": ["warn", "always"]
  }
}
